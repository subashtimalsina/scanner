<?xml version='1.0' encoding='utf-8'?>
<widget id="" version="1.2.92" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>WellnessXperts Cafe Smarthub</name>
    <description>
        Wizworks Smarthub App
    </description>
    <author email="wellnessxperts.hk@gmail.com" href="http://www.wellnessnetwork.me">
        WellnessXperts
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <gap:plugin name="com.phonegap.plugin.statusbar" />
    <preference name="EnableViewportScale" value="true" />
    <preference name="SplashScreenDelay" value="0" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="FadeSplashScreenDuration" value="0" />
</widget>
